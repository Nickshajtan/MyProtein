{
    "name": "hcc-wordpress/hcc",
    "description": "HCC start framework for Wordpress",
    "type": "wordpress-theme",
    "version": "v1.5.0",
    "authors": [
        {
            "name": "Nikita Nosov",
            "email": "shajtanuch@gmail.com"
        }
    ],
    "require": {
      "php": ">=7.1",
      "composer/installers": "~1.0",
      "illuminate/support": "5.6.*",
      "soberwp/controller": "~2.1.0"
    },
    "require-dev": {
      "squizlabs/php_codesniffer": "^2.8.0"
    },
    "repositories": [
      {
         "type": "vcs",
         "url": "https://github.com/Nickshajtan/MyProtein.git" 
      }
    ], 
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Helpers\\": "includes/classes/app/helpers",
            "Controllers\\": "includes/classes/app/controllers",
            "Modules\\": "includes/classes/app/modules",
            "Libs\\": "includes/classes/app/libs",
            "Core\\": "includes/classes/app/core",
            "App\\": "includes/classes/app"
        },
        "files": [
            "includes/classes/app/config.php"
        ]
    }
}
